(ns testboard.handler
  (:use compojure.core)
  (:require [compojure.handler :as handler]
            [compojure.route :as route]
            [cheshire.core :refer :all]))



(defroutes app-routes
  (GET "/" [] "Hello World")
  (POST "/postit" []
        (generate-string {:test {:name "ad"}}))
  (route/resources "/")
  (route/not-found "Not Found"))

(def app
  (handler/site app-routes))

